<Defs>
	<VREAndroids.AndroidGeneDef ParentName="VREA_HardwareBase">
		<defName>AG_WorkerDroidHead</defName>
		<label>worker droid head</label>
		<description>Androids with this hardware have a completely inhuman, robotic face.</description>
		<iconPath>UI/Icons/Genes/AG_RoboticFace1Icon</iconPath>
		<displayOrderInCategory>1000</displayOrderInCategory>
		<biostatCpx>0</biostatCpx>
		<isCoreComponent>false</isCoreComponent>
		<exclusionTags>
			<li>AG_Heads</li>
			<li>Heads</li>
			<li>Head</li>
		</exclusionTags>
		<beardTagFilter>
			<whitelist>true</whitelist>
			<tags>
				<li>NoBeard</li>
			</tags>
		</beardTagFilter>
		<hairTagFilter>
			<whitelist>true</whitelist>
			<tags>
				<li>Bald</li>
			</tags>
		</hairTagFilter>
		<customEffectDescriptions>
			<li>Using this with other cosmetic genes may look weird.</li>
		</customEffectDescriptions>
		<graphicData>
			<layer>PostHair</layer>
			<drawLoc>HeadTop</drawLoc>
		</graphicData>
		<forcedHeadTypes>
			<li>AG_SyntheticWorkerMale</li>
			<li>AG_SyntheticWorkerFemale</li>
		</forcedHeadTypes>
		<modExtensions Inherit="False">
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathXenogenes>UI/Icons/Genes/GeneBackground_Hardware</backgroundPathXenogenes>
				<backgroundPathEndogenes>UI/Icons/Genes/GeneBackground_Hardware</backgroundPathEndogenes>
			</li>
		</modExtensions>
	</VREAndroids.AndroidGeneDef>
