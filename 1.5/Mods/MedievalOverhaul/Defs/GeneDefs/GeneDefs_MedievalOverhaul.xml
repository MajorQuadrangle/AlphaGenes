<?xml version="1.0" encoding="utf-8" ?>
<Defs>




	<GeneDef ParentName="GeneHeadboneBase">
		<defName>AG_RoxHorns</defName>
		<label>rox horns</label>
		<displayCategory>AG_Cosmetic_Horns</displayCategory>
		<description>Carriers of this gene grow two large curved horns protruding from the forehead.</description>
		<iconPath>UI/Icons/Genes/AG_RoxHorns</iconPath>
		<selectionWeight>0</selectionWeight>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/AG_RoxHorns/AG_RoxHorns</graphicPath>
			<color>(255, 255, 255)</color>
			<layer>PostHeadgear</layer>
		</graphicData>
		<customEffectDescriptions>
			<li>Blessed by the Rox.</li>
		</customEffectDescriptions>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>rox</symbol>
				</li>
			
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>rox</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
	
	<GeneDef>
		<defName>AG_RoxPelt</defName>
		<label>rox pelt</label>
		<description>Carriers of this gene have a luxurious mat of grey hair covering their upper torso.</description>
		<iconPath>UI/Icons/Genes/AG_RoxPelt</iconPath>
		<biostatCpx>1</biostatCpx>
		<displayCategory>Miscellaneous</displayCategory>
		<statOffsets>
      <ComfyTemperatureMin>-10</ComfyTemperatureMin>
    </statOffsets>
		<biostatMet>-1</biostatMet>
		<selectionWeight>0</selectionWeight>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>rox</symbol>
				</li>
			
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>rox</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/BodyAttachments/AG_RoxPelt/AG_RoxPelt</graphicPath>
			<drawScale>2</drawScale>
			<drawOffsetNorth>(0, 1, 0)</drawOffsetNorth>
			<drawOffsetSouth>(0, 1, 0)</drawOffsetSouth>
			<drawOffsetEast>(0, 1, 0)</drawOffsetEast>
			<drawLoc>Tailbone</drawLoc>
			<colorType>Hair</colorType>
		</graphicData>

		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>

			</li>
		</modExtensions>
	</GeneDef>







</Defs>