<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<!-- Ears -->
	<GeneDef ParentName="GeneEarsBase">
		<defName>AG_RadYakEars</defName>
		<label>radyak ears</label>
		<selectionWeight>0</selectionWeight>
		<description>Carriers of this gene will grow pointed radyak ears.</description>
		<iconPath>UI/Icons/Genes/AG_RadYakEarsIcon</iconPath>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/AG_RadYakEars/AG_RadYakEars</graphicPath>
			 <colorType>Skin</colorType>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>radyak</symbol>
				</li>
				<li>
					<symbol>rad</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>yak</symbol>
				</li>
				<li>
					<symbol>ear</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
	
	
