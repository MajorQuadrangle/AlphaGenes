	<!-- Lower Antennae -->
	<GeneDef Name="AG_GeneLowerAntennaBase" Abstract="True">
		<biostatCpx>0</biostatCpx>
		<endogeneCategory>Headbone</endogeneCategory>
		<displayCategory>Cosmetic</displayCategory>
		<displayOrderInCategory>110</displayOrderInCategory>
		
		<exclusionTags>
			
			<li>AG_LowerAntennae</li>
		</exclusionTags>
		<graphicData>
			<layer>PostHeadgear</layer>
			<drawLoc>HeadTop</drawLoc>
			<colorType>Skin</colorType>
		</graphicData>
		<randomChosen>true</randomChosen>
	</GeneDef>
	
	<GeneDef ParentName="AG_GeneLowerAntennaBase">
		<defName>AG_NoLowerAntennae</defName>
		<label>no lower antennae</label>
		<description>Carriers of this gene don't have lower antennae.</description>
		<customEffectDescriptions>
			<li>With this gene you can create xenotypes in which a pawn can have lower antennae or a lack of them at random</li>
		</customEffectDescriptions>
		<iconPath>UI/Icons/Genes/AG_NoLowerAntennae</iconPath>
		<biostatCpx>0</biostatCpx>
		<displayCategory>AG_Cosmetic_Horns</displayCategory>
		<displayOrderInCategory>-998</displayOrderInCategory>
		<selectionWeight>0</selectionWeight>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
	
	<GeneDef ParentName="AG_GeneLowerAntennaBase">
		<defName>AG_LowerAntennas</defName>
		<label>lower antennae</label>
		<displayCategory>AG_Cosmetic_Horns</displayCategory>
		<description>Carriers of this gene grow two large antennae protruding from their jaw.</description>
		<iconPath>UI/Icons/Genes/AG_LowerAntennas</iconPath>
		<selectionWeight>0</selectionWeight>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/AG_LowerAntennas/AG_LowerAntennas</graphicPath>
			 <visibleNorth>false</visibleNorth>
		</graphicData>
		
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
</Defs>